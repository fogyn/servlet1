      ServletCalculate
    </servlet-class>

  </servlet>

  <servlet-mapping>
    <servlet-name>calculate</servlet-name>
    <url-pattern>/mvnweb_war_exploded/calculate</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>
      test
    </servlet-name>
    <servlet-class>
      ServletTest
    </servlet-class>

  </servlet>

  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/mvnweb_war_exploded/test</url-pattern>
  </servlet-mapping>
</web-app>
