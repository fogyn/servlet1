    <servlet-class>
        Servlet1
    </servlet-class>

  </servlet>

  <servlet-mapping>
    <servlet-name>S1</servlet-name>
    <url-pattern>/servlet1</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>
      Basic
    </servlet-name>
    <servlet-class>
      ServletBasic
    </servlet-class>

  </servlet>

  <servlet-mapping>
    <servlet-name>Basic</servlet-name>
    <url-pattern>/mvnweb_war_exploded/basic</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>
