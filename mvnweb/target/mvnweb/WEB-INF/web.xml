  </servlet>

  <servlet-mapping>
    <servlet-name>text</servlet-name>
    <url-pattern>/mvnweb_war_exploded/text</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>
      calculate
    </servlet-name>
